{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "storm-ui",
    "labels": {
      "name": "storm-ui"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "storm-ui",
        "image": "mattf/storm-ui",
        "ports": [
          {
            "name": "storm-ui-http",
            "containerPort": 8080
          }
        ],
        "resources": {
          "limits": {
            "cpu": "100m"
          }
        }
      }
    ]
  }
}
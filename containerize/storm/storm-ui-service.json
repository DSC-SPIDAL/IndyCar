{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "storm-ui",
    "labels": {
      "name": "storm-ui"
    }
  },
  "spec": {
    "ports": [
      {
        "port": 8080,
        "targetPort": "storm-ui-http",
        "protocol": "TCP"
      }
    ],
    "selector": {
      "name": "storm-ui"
    },
    "type": "LoadBalancer"
  }
}